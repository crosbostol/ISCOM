{
    "name": "backend",
    "version": "1.0.0",
    "description": "RESTful API with Node.js, Express, and PostgreSQL",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "nodemon src/server.ts",
        "migrate": "node-pg-migrate up -m src/data/migrations",
        "seed": "ts-node src/data/seed.ts",
        "seed:prod": "node dist/data/seed.js",
        "test": "jest",
        "db:dump-schema": "dotenv -- cross-var pg_dump -s -x -O --no-comments $DATABASE_URL > db/structure.sql"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/multer": "^2.0.0",
        "bcryptjs": "^3.0.3",
        "chart.js": "^4.3.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "csurf": "^1.11.0",
        "csv-parser": "^3.2.0",
        "express": "^4.18.2",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.3",
        "morgan": "^1.10.1",
        "multer": "^2.0.2",
        "node-pg-migrate": "^8.0.3",
        "pg": "^8.16.3",
        "startbootstrap-sb-admin-2": "^4.1.4",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@types/bcryptjs": "^3.0.0",
        "@types/cors": "^2.8.19",
        "@types/dotenv": "^8.2.3",
        "@types/express": "^5.0.6",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/morgan": "^1.9.10",
        "@types/node": "^24.10.4",
        "@types/node-pg-migrate": "^2.3.1",
        "@types/pg": "^8.15.6",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "cross-var": "^1.1.0",
        "dotenv": "^17.2.3",
        "dotenv-cli": "^11.0.0",
        "jest": "^30.2.0",
        "nodemon": "^2.0.21",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    }
